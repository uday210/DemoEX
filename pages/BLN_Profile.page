<script>

   //checking  dropzone image is empty or not
    emptyDropznImage(); 
    function emptyDropznImage() {
        var userimage = '{!GNUSER.User_Pic__c}';        
        if(!userimage){
           
                $("#simagediv img").hide();
                      
}
