      <script>

 var elems = document.getElementsByTagName ( "input" );
    var len = elems.length;

    for ( var i = 0; i < len; i++ )
    {
        if ( elems[i].type == "checkbox" )
        {
            if(elems[i].checked == true)
            {
               var currentRow = elems[i].parentNode.parentNode;
               currentRow.setAttribute("class", "selected");
            }
        }
    }
